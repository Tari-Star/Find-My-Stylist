<ol class="post-list">
  {{#each posts as |post|}}
  <li>
    <article class="post">
      <div class="title">
        <a href="{{post.post_url}}" target="_blank">{{post.title}}</a>
        <span>({{post.post_url}})</span>
      </div>
      <div class="meta">
        Created by {{post.user.username}} on
        {{post.created_at}}
      </div>
    </article>
  </li>
  {{/each}}
</ol>